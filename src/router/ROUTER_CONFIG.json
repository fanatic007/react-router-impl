{"routes":[
  {
    "path": "login",
    "name": "Login",
    "locale": "user.login",
    "component": "pages/login",
    "hideInMenu": true
  },
  {
    "path": "dashboard",
    "locale": "dashboard",
    "name": "Dashboard",
    "icon": "dashboard",
    "component": "pages/dashboard",
    "index": true,
    "routes": [
      {
        "path": "analysis",
        "locale": "dashboard.analysis",
        "component": "pages/dashboard/analysis",
        "name": "Analysis",
        "accessTO": ["admin"]
      },
      {
        "path": "monitor",
        "locale": "dashboard.monitor",
        "component": "pages/dashboard/monitor",
        "name": "Monitor"
      },
      {
        "path": "workplace",
        "locale": "dashboard.workplace",
        "component": "pages/dashboard/workplace",
        "name": "Workplace"
      }
    ]
  },
  {
    "path": "projects",
    "locale": "projects",
    "name": "Projects",
    "icon": "projects",
    "redirect": "list",
    "routes": [
      { 
        "path": "list",
        "locale": "projects.list", 
        "name": "Projects", 
        "icon": "projects",
        "component": "pages/projects/list", 
        "exact": true 
      },
      {
        "path": ":id",
        "locale": "projects.details",
        "name": "Project Details",
        "component": "pages/projects/details", 
        "hideInMenu": true,
        "icon": "projects",
        "key": "projects",
        "exact": true,
        "routes": [
          {
            "path": "settings", 
            "locale": "projects.settings",
            "component":"pages/projects/settings",
            "icon": "settings",
            "name": "Settings", 
            "parentKey": "details", 
            "exact": true
          }
        ]
      }
    ]
  },
  { "path": "*", "component": "pages/404", "hideInMenu":true }
]
}